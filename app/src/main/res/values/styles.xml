<resources>
    <style name="AppTheme" parent="Theme.AppCompat.Light.NoActionBar">
        <item name="colorPrimary">@color/colorPrimary</item>
        <item name="colorPrimaryDark">@color/colorPrimaryDark</item>
        <item name="colorAccent">@color/colorAccent</item>
        <item name="drawerArrowStyle">@style/WhiteDrawerIconStyle</item>
    </style>

    <style name="WhiteDrawerIconStyle" parent="Widget.AppCompat.DrawerArrowToggle">
        <item name="spinBars">true</item>
        <item name="color">@android:color/white</item>
    </style>
    <!-- Base application theme. -->
    <!--<style name="AppTheme" parent="Base.Theme.AppCompat.Light.DarkActionBar">-->
        <!--&lt;!&ndash; Customize your theme here. &ndash;&gt;-->
       <!---->
    <!--</style>-->

    <style name="RatingBar" parent="Theme.AppCompat">
        <item name="colorControlNormal">#d89d1d</item>
        <item name="colorControlActivated">#d89d1d</item>
    </style>

    <style name="AppTheme.ProgressBar" parent="@style/Theme.AppCompat">
        <item name="colorAccent">@color/Black</item>
    </style>

    <style name="AppTheme.ProgressBarWhite" parent="@style/Theme.AppCompat">
        <item name="colorAccent">@color/white</item>
    </style>
    <style name="AppTheme2" parent="Base.Theme.AppCompat.Light.DarkActionBar">

        <item name="colorPrimary">@color/colorPrimary</item>
        <item name="colorPrimaryDark">@color/colorPrimaryDark</item>
        <item name="colorAccent">@color/colorAccent</item>
    </style>

    <style name="AppTheme1" parent="Theme.AppCompat.Light.NoActionBar">
    </style>
    <style name="AppTheme.NoActionBar">
        <item name="windowActionBar">false</item>
        <item name="windowNoTitle">true</item>
    </style>

    <style name="AppTheme.AppBarOverlay"></style>

    <style name="AppTheme.PopupOverlay"></style>


</resources>
